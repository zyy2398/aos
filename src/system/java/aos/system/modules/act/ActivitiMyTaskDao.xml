<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 缓存相关映射SQL语句 -->
<mapper namespace="ActivitiMyTask">
    <update id="updateProcInsIdByBusinessId" parameterType="Dto">
        UPDATE ${businessTable} SET
			instance_id = #{procInsId}
		WHERE id = #{businessId}
    </update>
</mapper>